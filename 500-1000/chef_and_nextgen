#include <stdio.h>

int main() {
	// your code goes here
	int t,a,b,x,y;
	int c,d;
	scanf("%d",&t);
	for(int i=0;i<t;i++)
	    {
	       scanf("%d%d%d%d",&a,&b,&x,&y);
	       c=a*b;
	       d=x*y;
	       if(d>=c)
	       {
	           printf("YES\n");
	       }
	       else
	       {
	           printf("NO\n");
	       }
	    }
	
	return 0;

}

